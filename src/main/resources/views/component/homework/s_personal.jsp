<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
              
         

          <div class="container-fluid">
			<div id="personal" class="form-horizontal">
				<div class="row">
	        <div class="col-xs-4 col-md-4 text-right">
	        	<label>用户名：</label>
	        </div>
	        <div class="col-xs-8 col-md-8 text-left">
	        		<span> {{username}}</span>
	        	</div>
        </div>
        <div class="row">
	        <div class="col-xs-4 col-md-4 text-right">
	        	<label>真实姓名：</label>
	        </div>
	        <div class="col-xs-8 col-md-8 text-left">
	        		<span > {{name}}</span>
	        </div>
        </div>
        <div class="row">
	        <div class="col-xs-4 col-md-4 text-right">
	        	<label>手机号：</label>
	        </div>
	        <div class="col-xs-8 col-md-8 text-left">
	       		 <span> {{phone}}</span>
	        </div>
        </div>
        <div class="row">
	        <div class="col-xs-4 col-md-4 text-right">
	        	<label>邮箱：</label>
	        </div>
	        <div class="col-xs-8 col-md-8 text-left">
	        		<span> {{email}}</span>
	        </div>
	    </div>
	    
        <div class="row">
	        <div class="col-xs-4 col-md-4 text-right">
	        	<label>所属学校：</label>
	        </div>
	        <div class="col-xs-8 col-md-8 text-left">
	        		<span> {{school_name}}</span>
			</div>
	    </div>
	    <div class="row">
	        <div class="col-xs-4 col-md-4 text-right">
	        	<label>所属班级：</label>
	        </div>
	        <div class="col-xs-8 col-md-8 text-left">
	        		<span> {{class_name}}</span>
			</div>
	    </div>
			</div>
          </div>
